define hook-stop
  call stopProgram()
  frame
end

define hook-continue
  call continueProgram()
end

define hook-finish
  call continueProgram()
end

define hook-advance
  call continueProgram()
end

define hook-next
  call continueProgram()
end

define hook-nexti
  call continueProgram()
end

define hook-step
  call continueProgram()
end

define hook-stepi
  call continueProgram()
end

define hook-signal
  call continueProgram()
end

define hook-until
  call continueProgram()
end

define hex
  x /$arg1xb $arg0
end

define dec
  x /$arg1ub $arg0
end

define bin
  x /$arg1tb $arg0
end

define string
  x/s $arg0
end

start
